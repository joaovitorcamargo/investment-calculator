    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.4.1",
